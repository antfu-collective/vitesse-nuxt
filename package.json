{
  "name": "@daotl/nuxt-starter",
  "version": "3.0.0",
  "description": "A Nuxt 3 + TypeScript starter template",
  "homepage": "https://github.com/daotl/fes-starter",
  "private": true,
  "packageManager": "pnpm@7.0.1",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "start": "node .output/server/index.mjs",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "prepare": "husky install"
  },
  "prettier": "@daotl/prettier-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/daotl/nuxt-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.5",
    "@iconify-json/twemoji": "^1.1.4",
    "pinia": "^2.0.14",
    "vue": "^3.2.34"
  },
  "devDependencies": {
    "@daotl/eslint-config-vue": "^0.3.25",
    "@nuxtjs/color-mode": "^3.0.3",
    "@pinia/nuxt": "^0.1.9",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@unocss/nuxt": "^0.33.5",
    "@vueuse/nuxt": "^8.5.0",
    "element-plus": "^2.2.0",
    "eslint": "^8.15.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "nuxt": "^3.0.0-rc.3",
    "nuxt-lodash": "^2.2.0",
    "nuxt-typed-router": "^1.1.2",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "stylelint": "^14.8.2",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.7.2",
    "vite": "^2.9.9"
  }
}
