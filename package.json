{
  "name": "@daotl/nuxt-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@7.19.0",
  "description": "A Nuxt 3 + TypeScript starter template",
  "homepage": "https://github.com/daotl/nuxt-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/daotl/nuxt-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/nuxt-starter/issues"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "build:lambda": "cross-env NITRO_PRESET=lambda nuxi build",
    "generate": "nuxi generate",
    "start": "node .output/server/index.mjs",
    "serverless:deploy": "serverless deploy",
    "serverless:domain": "serverless create_domain",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "postinstall": "nuxi prepare",
    "cz": "npx cz"
  },
  "config": {
    "commitizen": {
      "path": "commitiquette"
    }
  },
  "prettier": "@daotl/prettier-config",
  "dependencies": {
    "element-plus": "^2.2.28",
    "pinia": "^2.0.29",
    "rxjs": "^7.8.0",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.1",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@daotl/eslint-config-vue": "^0.3.33",
    "@iconify-json/carbon": "^1.1.14",
    "@iconify-json/twemoji": "^1.1.10",
    "@nuxtjs/color-mode": "^3.2.0",
    "@pinia/nuxt": "^0.4.6",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.11.18",
    "@unocss/nuxt": "^0.49.2",
    "@vueuse/nuxt": "^9.12.0",
    "commitiquette": "^1.2.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.33.0",
    "lefthook": "^1.2.8",
    "nuxt": "^3.1.1",
    "nuxt-typed-router": "^2.2.1",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.3",
    "sass": "^1.58.0",
    "stylelint": "^14.16.1",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "git://github.com/daotl/unplugin-auto-import#6b2bbc40c928340fb473b56d91240d9d617665c4",
    "unplugin-vue-components": "git://github.com/daotl/unplugin-vue-components#75d581e694b62072d3ab7dcae496aff91432f74b",
    "vite": "^4.0.4",
    "vue-tsc": "^1.0.24"
  }
}
