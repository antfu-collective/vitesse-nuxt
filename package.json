{
  "name": "@daotl/nuxt-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@7.1.7",
  "description": "A Nuxt 3 + TypeScript starter template",
  "homepage": "https://github.com/daotl/fes-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/daotl/nuxt-starter.git"
  },
  "scripts": {
    "build": "nuxi build",
    "build:lambda": "cross-env NITRO_PRESET=lambda nuxi build",
    "dev": "nuxi dev",
    "graphql-codegen": "graphql-codegen",
    "start": "node .output/server/index.mjs",
    "serverless:deploy": "serverless deploy",
    "serverless:domain": "serverless create_domain",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "prepare": "husky install"
  },
  "prettier": "@daotl/prettier-config",
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.5",
    "@iconify-json/twemoji": "^1.1.4",
    "@urql/vue": "^0.6.2",
    "apollo-server-core": "^3.8.2",
    "class-validator": "^0.13.2",
    "graphql": "^15.8.0",
    "graphql-playground-html": "^1.6.30",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.0.14",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@daotl/eslint-config-vue": "^0.3.25",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.12",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-vue-apollo": "^3.2.12",
    "@graphql-codegen/typescript-vue-urql": "^2.2.12",
    "@intlify/nuxt3": "^0.2.2",
    "@nuxtjs/color-mode": "^3.1.3",
    "@pinia/nuxt": "^0.1.9",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@unocss/nuxt": "^0.37.4",
    "@vueuse/nuxt": "^8.6.0",
    "element-plus": "^2.2.2",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "nuxt": "^3.0.0-rc.3",
    "nuxt-lodash": "^2.2.0",
    "nuxt-typed-router": "^1.1.2",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.2",
    "sass": "^1.52.2",
    "stylelint": "^14.8.5",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.7.3",
    "unplugin-auto-import": "^0.8.7",
    "vite": "^2.9.9"
  },
  "pnpm": {
    "overrides": {
      "jiti": "npm:@daotl/jiti@^1.13.1"
    }
  }
}
