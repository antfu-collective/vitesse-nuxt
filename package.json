{
  "name": "@daotl/nuxt-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@7.1.7",
  "description": "A Nuxt 3 + TypeScript starter template",
  "homepage": "https://github.com/daotl/fes-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/daotl/nuxt-starter.git"
  },
  "scripts": {
    "build": "nuxi build",
    "build:lambda": "cross-env NITRO_PRESET=lambda nuxi build",
    "dev": "nuxi dev",
    "graphql-gen": "npm run graphql-gen-ops && npm run graphql-codegen",
    "graphql-gen-ops": "gqlg --schemaFilePath ./graphql/generated/schema.gql --destDirPath ./graphql/generated/ops --depthLimit 100",
    "graphql-codegen": "graphql-codegen -c ./graphql/codegen.yml",
    "start": "node .output/server/index.mjs",
    "serverless:deploy": "serverless deploy",
    "serverless:domain": "serverless create_domain",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "prepare": "husky install"
  },
  "prettier": "@daotl/prettier-config",
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@iconify-json/twemoji": "^1.1.5",
    "@urql/vue": "^0.6.3",
    "apollo-server-core": "^3.10.2",
    "class-validator": "^0.13.2",
    "element-plus": "^2.2.5",
    "graphql": "^15.3.0",
    "graphql-playground-html": "^1.6.30",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.0.22",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-tsc": "^0.40.10"
  },
  "devDependencies": {
    "@daotl/eslint-config-vue": "^0.3.25",
    "@daotl/unplugin-vue-components": "^0.19.6",
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/near-operation-file-preset": "^2.4.1",
    "@graphql-codegen/typed-document-node": "^2.3.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-vue-apollo": "^3.3.3",
    "@graphql-codegen/typescript-vue-urql": "^2.3.3",
    "@nuxtjs/color-mode": "^3.1.4",
    "@pinia/nuxt": "^0.4.1",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.7.15",
    "@unocss/nuxt": "^0.45.12",
    "@vueuse/nuxt": "9.1.1",
    "eslint": "^8.23.0",
    "gql-generator": "^1.0.17",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.8",
    "nuxt-typed-router": "^1.2.2",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "stylelint": "^14.11.0",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.8.2",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.1.0"
  }
}
