{
  "name": "@daotl/nuxt-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@7.19.0",
  "description": "A Nuxt 3 + TypeScript starter template",
  "homepage": "https://github.com/daotl/nuxt-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/daotl/nuxt-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/nuxt-starter/issues"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "build:lambda": "cross-env NITRO_PRESET=lambda nuxi build",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "serverless:deploy": "serverless deploy",
    "serverless:domain": "serverless create_domain",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public",
    "cz": "npx cz"
  },
  "config": {
    "commitizen": {
      "path": "commitiquette"
    }
  },
  "prettier": "@daotl/prettier-config",
  "dependencies": {
    "element-plus": "^2.3.0",
    "pinia": "^2.0.33",
    "rxjs": "^7.8.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@daotl/eslint-config-vue": "^0.3.35",
    "@element-plus/nuxt": "^1.0.4",
    "@iconify-json/carbon": "^1.1.16",
    "@iconify-json/twemoji": "^1.1.11",
    "@nuxtjs/color-mode": "^3.2.0",
    "@pinia/nuxt": "^0.4.7",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.15.3",
    "@unocss/nuxt": "^0.50.4",
    "@vite-pwa/nuxt": "^0.0.7",
    "@vueuse/nuxt": "^9.13.0",
    "commitiquette": "^1.2.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.36.0",
    "lefthook": "^1.3.4",
    "nuxt": "^3.2.3",
    "nuxt-typed-router": "^3.0.6",
    "pinia": "^2.0.33",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.4",
    "sass": "^1.59.2",
    "stylelint": "^15.2.0",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "git://github.com/daotl/unplugin-auto-import#6b2bbc40c928340fb473b56d91240d9d617665c4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  }
}
